{
    "name": "rector/composer-plugin",
    "type": "composer-plugin",
    "description": "Detect package version changes and upgrade code on composer update",
    "license": "proprietary",
    "require": {
        "php": ">=7.1",
        "composer-plugin-api": "^1.1 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "2.2",
        "composer/xdebug-handler": "^2.0",
        "symplify/easy-coding-standard": "^10.0",
        "symplify/phpstan-extensions": "^10.0.10",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5",
        "tracy/tracy": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Rector\\ComposerPlugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\ComposerPlugin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check --verbose --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --verbose --ansi",
        "phpstan": "vendor/bin/phpstan analyse --ansi"
    },
    "repositories": [
        {
            "type": "path",
            "version": "dev-main",
            "url": "."
        }
    ],
    "extra": {
        "class": "Rector\\ComposerPlugin\\Plugin",
        "branch-alias": {
            "dev-main": "0.11.x-dev"
        }
    }
}
